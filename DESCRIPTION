Package: gom
Title: Grade of Membership Mixture Models
Version: 0.0.1
Authors@R: c(
    person("Jeferson", "Andrade", role = c("aut", "cre"), email = "jeferson@epopea.com.br", comment = c(ORCID = "0000-0002-0731-3427")),
